    <tr>
    <?php if($files = Files::where('filename','=', $value)->first()): ?>
    <td><?php echo $files->created_at; ?></td><td> <?php echo link_to_route('profile-filecomment', basename($value),array('fileid'=>$files->id )); ?><?php endif; ?></td>
          
    </tr>
     <?php endforeach; ?> 
</table></div>

<?php $__env->stopSection(); ?>

